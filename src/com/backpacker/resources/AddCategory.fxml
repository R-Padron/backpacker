<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchor-pane" stylesheets="@css/addCategory.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.backpacker.controllers.AddCategoryController">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0" styleClass="pane" stylesheets="@css/addCategory.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="addCatText" layoutX="260.0" layoutY="270.0" prefHeight="31.0" prefWidth="244.0">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <Label layoutX="29.0" layoutY="272.0" text="Enter new category name:" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <effect>
                  <DropShadow />
               </effect></Label>
            <Button fx:id="enterButton" layoutX="260.0" layoutY="333.0" mnemonicParsing="false" onAction="#enterClick" text="Enter">
               <effect>
                  <DropShadow />
               </effect></Button>
            <ImageView fitHeight="230.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
         </children></Pane>
   </children>
</AnchorPane>
