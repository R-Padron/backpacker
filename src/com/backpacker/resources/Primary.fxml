            <TabPane fx:id="invTabPane" onKeyPressed="#tapPaneKeyPress" onMouseClicked="#tabPaneClick" prefHeight="700.0" prefWidth="1100.0" styleClass="indexed-cell" stylesheets="@css/table.css" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
